<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginFormController">
   <children>
      <Label alignment="CENTER" layoutX="-1.0" layoutY="14.0" prefHeight="43.0" prefWidth="600.0" text="LOGIN" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <font>
            <Font name="Cantarell Extra Bold" size="54.0" />
         </font></Label>
      <FlowPane alignment="CENTER" layoutX="72.0" layoutY="88.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Label prefHeight="29.0" prefWidth="223.0" text="Don't have an account?">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
            <Label id="btnRegisterHere" onMouseClicked="#lblRegisterHereOnMouseClicked" stylesheets="@../CSS/LoginForm.css" text="Rgister Here">
               <font>
                  <Font name="Cantarell Extra Bold" size="20.0" />
               </font>
            </Label>
         </children>
      </FlowPane>
      <Label alignment="CENTER" layoutY="162.0" prefHeight="18.0" prefWidth="263.0" text="Enter your NIC" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <TextField fx:id="txtNIC" layoutX="201.0" layoutY="200.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" />
      <Button id="btnLogin" fx:id="btnLogin" layoutX="274.0" layoutY="271.0" mnemonicParsing="false" onAction="#btnLoginOnAction" prefHeight="31.0" prefWidth="77.0" stylesheets="@../CSS/LoginForm.css" text="Login" AnchorPane.leftAnchor="250.0" AnchorPane.rightAnchor="250.0">
         <font>
            <Font name="Cantarell Extra Bold" size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
